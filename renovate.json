{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>plopoyop/renovate-config",
    "github>plopoyop/renovate-config:kubernetes-ansible-collection",
    "github>plopoyop/renovate-config:automerge-devbox"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "helm"
      ],
      "matchPackageNames": [
        "zammad"
      ],
      "registryUrls": [
        "https://zammad.github.io/zammad-helm"
      ]
    },
    {
      "matchDatasources": [
        "github-releases"
      ],
      "matchPackageNames": [
        "tweedegolf/mailcrab"
      ]
    },
    {
      "matchDatasources": [
        "helm"
      ],
      "matchPackageNames": [
        "gatus"
      ],
      "registryUrls": [
        "https://twin.github.io/helm-charts"
      ]
    },
    {
      "matchDatasources": [
        "helm"
      ],
      "matchPackageNames": [
        "mongodb-kubernetes"
      ],
      "registryUrls": [
        "https://mongodb.github.io/helm-charts"
      ]
    },{
      "matchDatasources": [
        "helm"
      ],
      "matchPackageNames": [
        "mongodb-instance"
      ],
      "registryUrls": [
        "https://plopoyop.github.io/charts"
      ]
    },
    {
      "matchDatasources": [
        "helm"
      ],
      "matchPackageNames": [
        "atlantis"
      ],
      "registryUrls": [
        "https://runatlantis.github.io/helm-charts"
      ]
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "Zammad\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "zammad",
      "autoReplaceStringTemplate": "Zammad         | v{{newValue}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/zammad/defaults/main.yml$/",
        "/roles/zammad/README.md$/"
      ],
      "matchStrings": [
        "zammad_helm_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "zammad",
      "autoReplaceStringTemplate": "zammad_helm_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/mailcrab/defaults/main.yml$/",
        "/roles/mailcrab/README.md$/"
      ],
      "matchStrings": [
        "mailcrab_github_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "tweedegolf/mailcrab",
      "autoReplaceStringTemplate": "mailcrab_github_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "Mailcrab\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "tweedegolf/mailcrab",
      "autoReplaceStringTemplate": "Mailcrab         | v{{newValue}}",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/gatus/defaults/main.yml$/",
        "/roles/gatus/README.md$/"
      ],
      "matchStrings": [
        "gatus_helm_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "gatus",
      "autoReplaceStringTemplate": "gatus_helm_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "Gatus\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "gatus",
      "autoReplaceStringTemplate": "Gatus         | v{{newValue}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/mongodb_operator/defaults/main.yml$/",
        "/roles/mongodb_operator/README.md$/"
      ],
      "matchStrings": [
        "mongodb_operator_helm_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "mongodb-kubernetes",
      "autoReplaceStringTemplate": "mongodb_operator_helm_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "MongoDb for Kubernetes\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "mongodb-kubernetes",
      "autoReplaceStringTemplate": "MongoDb for Kubernetes | v{{newValue}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/mongodb_instance/defaults/main.yml$/",
        "/roles/mongodb_instance/README.md$/"
      ],
      "matchStrings": [
        "mongodb_instance_crd_helm_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "mongodb-instance",
      "autoReplaceStringTemplate": "mongodb_instance_crd_helm_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "MongoDb Instance\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "mongodb-instance",
      "autoReplaceStringTemplate": "MongoDb Instance | v{{newValue}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/roles/atlantis/defaults/main.yml$/",
        "/roles/atlantis/README.md$/"
      ],
      "matchStrings": [
        "atlantis_helm_version:\\s*(?<quote>\")?(?<currentValue>[0-9\\.]+)\"?"
      ],
      "depNameTemplate": "atlantis",
      "autoReplaceStringTemplate": "atlantis_helm_version: {{#if quote}}\"{{newValue}}\"{{else}}{{newValue}}{{/if}}",
      "datasourceTemplate": "helm"
    },
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^README.md$/"
      ],
      "matchStrings": [
        "Atlantis\\s*\\|\\s*v?(?<currentValue>[0-9\\.]+)"
      ],
      "depNameTemplate": "atlantis",
      "autoReplaceStringTemplate": "Atlantis | v{{newValue}}",
      "datasourceTemplate": "helm"
    }
  ]
}
